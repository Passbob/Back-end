<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="selectAllProduct">
        SELECT *
        FROM PRODUCT;
    </entry>
    <entry key="updateProductCount">
        UPDATE PRODUCT
        SET PRODUCT_COUNT = ?
        WHERE PRODUCT_CODE = ?;
    </entry>
    <entry key="selectRestCount">
        SELECT
            PRODUCT_NAME,
            PRODUCT_COUNT
        FROM PRODUCT;
    </entry>
    <entry key="deleteProductInfo">
        DELETE
        FROM  PRODUCT
        WHERE PRODUCT_CODE = ?;
    </entry>
    <entry key="insertProduct">
        INSERT INTO PRODUCT VALUES
        ( ?, ?, ?, ?);
    </entry>
</properties>